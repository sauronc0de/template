{
    "version": 3,
    "cmakeMinimumRequired": {
      "major": 3,
      "minor": 21,
      "patch": 1
    },
    "configurePresets": [
      {
        "name": "default",
        "hidden": false,
        "binaryDir": "${sourceDir}/build/${presetName}",
        "cacheVariables": {
